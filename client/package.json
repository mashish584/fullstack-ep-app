{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "pretty": "prettier --write --config .prettierrc.json  **/**.{ts,tsx}",
    "lint": "eslint  **/**.{ts,tsx}",
    "lint:fix": "eslint **/**.{ts,tsx} --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=true npm run build",
    "analyze-code": "npm run pretty && npm run lint:fix && npm run lint"
  },
  "dependencies": {
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.23.2",
    "husky": "^6.0.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  }
}
